{
  "manifest_version": 3,
  "name": "Gmail Phishing Detection",
  "version": "0.1",
  "description": "A chrome extension designed to scan a user's Gmail inbox for potential phishing emails",
  "permissions": [
  ],
  "host_permissions": [
    "https://gmail.googleapis.com/",
    "http://localhost:8000/"
  ],
  "background": {
    "service_worker": "emailParser.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  }
}